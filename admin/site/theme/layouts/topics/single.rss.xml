{{- "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"yes\"?>" | safeHTML }}
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ .Params.Title | transform.XMLEscape | safeHTML }}</title>
    <link>{{ .Permalink | transform.XMLEscape | safeHTML }}</link>
    <description>AFP entries in {{ .Params.name | transform.XMLEscape | safeHTML }}</description>
    {{- range .Params.entries }}
      {{- range .entries }}
        {{- partial "entry.rss.xml" (page.GetPage (printf "/entries/%s" .)) }}
      {{- end }}
    {{- end }}
  </channel>
</rss>